{{#paper-toolbar}}
  <h2 class="md-toolbar-tools">
    {{#paper-sidenav-toggle class="menu-sidenav-toggle"}}
      {{paper-icon icon="menu"}}
    {{/paper-sidenav-toggle}}
    <span>Lists</span>
  </h2>
{{/paper-toolbar}}

{{#paper-content classNames="md-padding"}}
<div class="doc-content">
  {{#paper-content classNames="md-whiteframe-z1 list-demo"}}
    {{#paper-list}}
      {{#each item in listData}}
        {{#paper-item}}
          {{#paper-tile-left}}
            <img {{bind-attr src=item.face alt=item.who}} class="face">
          {{/paper-tile-left}}

          {{#paper-tile-content}}
            <h3>{{item.what}}</h3>
            <h4>{{item.who}}</h4>
            <p>
              {{item.notes}}
            </p>
          {{/paper-tile-content}}
        {{/paper-item}}
      {{/each}}
    {{/paper-list}}
  {{/paper-content}}

  <h3>Template</h3>
{{#code-block language='handlebars'}}
\{{#paper-content classNames="md-whiteframe-z1 list-demo"}}
  \{{#paper-list}}
    \{{#each item in listData}}
      \{{#paper-item}}
        \{{#paper-tile-left}}
          &lt;img \{{bind-attr src=item.face alt=item.who}} class=&quot;face&quot;&gt;
        \{{/paper-tile-left}}

        \{{#paper-tile-content}}
          &lt;h3&gt;\{{item.what}}&lt;/h3&gt;
          &lt;h4&gt;\{{item.who}}&lt;/h4&gt;
          &lt;p&gt;
            \{{item.notes}}
          &lt;/p&gt;
        \{{/paper-tile-content}}
      \{{/paper-item}}
    \{{/each}}
  \{{/paper-list}}
\{{/paper-content}}{{/code-block}}
</div>
{{/paper-content}}
