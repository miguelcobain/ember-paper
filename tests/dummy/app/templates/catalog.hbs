          @onChange={{fn (mut this.tabsSelectedTab)}}
          @selected={{this.tabsSelectedTab}}
          as |tabs|
        >
          <tabs.tab>
            Page One
          </tabs.tab>
          <tabs.tab @disabled={{true}}>
            Page Two (disabled)
          </tabs.tab>
          <tabs.tab>
            Page Three
          </tabs.tab>
          <tabs.tab>
            Page Four
          </tabs.tab>
          <tabs.tab>
            Page Five
          </tabs.tab>
        </PaperTabs>

        <p><code>centered</code></p>
      </div>

      <div>
        <PaperTabs
          @stretch={{true}}
          @onChange={{fn (mut this.tabsSelectedTab)}}
          @selected={{this.tabsSelectedTab}}
          as |tabs|
        >
          <tabs.tab>
            Page One
          </tabs.tab>
          <tabs.tab @disabled={{true}}>
            Page Two (disabled)
          </tabs.tab>
          <tabs.tab>
            Page Three
          </tabs.tab>
          <tabs.tab>
            Page Four
          </tabs.tab>
          <tabs.tab>
            Page Five
          </tabs.tab>
        </PaperTabs>

        <p><code>stretched</code></p>
      </div>

      <div>
        <PaperTabs
          @borderBottom={{true}}
          @center={{false}}
          @onChange={{fn (mut this.tabsSelectedTab)}}
          @selected={{this.tabsSelectedTab}}
          @stretch={{false}}
          as |tabs|
        >
          <tabs.tab>
            Page One
          </tabs.tab>
          <tabs.tab @disabled={{true}}>
            Page Two (disabled)
          </tabs.tab>
          <tabs.tab>
            Page Three
          </tabs.tab>
          <tabs.tab>
            Page Four
          </tabs.tab>
          <tabs.tab>
            Page Five
          </tabs.tab>
        </PaperTabs>

        <p><code>bottom border</code></p>
      </div>

      <div>
        <PaperTabs
          @selected={{this.tabsCurrentRouteName}}
          @borderBottom={{true}}
          @onChange={{null}}
          as |tabs|
        >
          <tabs.tab @value='catalog.index' @href={{href-to 'catalog.index'}}>
            Index
          </tabs.tab>
          <tabs.tab
            @value='catalog.nested-route'
            @href={{href-to 'catalog.nested-route'}}
          >
            Nested Route
          </tabs.tab>
        </PaperTabs>

        <LiquidOutlet class='md-padding' />
        <p><code>nested routing</code></p>
      </div>
    </card.content>
  </PaperCard>

  <PaperCard as |card|>
    <card.title>
      <h2>Toast</h2>
    </card.title>
    <card.content>
      <div id='toast-catalog' class='toast-catalog'></div>

      <PaperToast
        @capsule={{false}}
        @class='toast-catalog--override-position'
        @duration={{this.toastDuration}}
        @onClose={{null}}
        @parent='.toast-catalog'
        @swipeToClose={{false}}
        as |toast|
      >
        <toast.text>default</toast.text>
        <PaperButton @accent={{true}} @onClick={{null}}>
          OK
        </PaperButton>
      </PaperToast>

      <PaperToast
        @capsule={{true}}
        @class='toast-catalog--override-position'
        @duration={{this.toastDuration}}
        @onClose={{null}}
        @parent='.toast-catalog'
        @swipeToClose={{false}}
        as |toast|
      >
        <toast.text>capsule</toast.text>
        <PaperButton @warn={{true}} @onClick={{null}}>
          CANCEL
        </PaperButton>
      </PaperToast>

      <PaperToast
        @duration={{this.toastDuration}}
        @onClose={{null}}
        @parent='.toast-catalog'
        @swipeToClose={{false}}
        @class='md-accent toast-catalog--override-position'
        as |toast|
      >
        <toast.text>@class="md-accent"</toast.text>
        <PaperButton @warn={{true}} @onClick={{null}}>
          Wow, much accent
        </PaperButton>
      </PaperToast>

      <PaperToast
        @duration={{this.toastDuration}}
        @onClose={{null}}
        @parent='.toast-catalog'
        @swipeToClose={{false}}
        @class='md-warn toast-catalog--override-position'
        as |toast|
      >
        <toast.text>@class="md-warn"</toast.text>
        <PaperButton @accent={{true}} @onClick={{null}}>
          UNDO
        </PaperButton>
      </PaperToast>
    </card.content>
  </PaperCard>

  <PaperCard as |card|>
    <card.title>
      <h2>Toolbar</h2>
    </card.title>
    <card.content>
      <PaperToolbar>
        <PaperToolbarTools>
          <PaperButton @iconButton={{true}}>
            <PaperIcon @icon='menu' />
          </PaperButton>
          <h2>
            Toolbar with Icon Buttons
          </h2>
          <span class='flex'></span>
          <PaperButton @iconButton={{true}}>
            <PaperIcon @icon='favorite' />
          </PaperButton>
          <PaperButton @iconButton={{true}}>
            <PaperIcon @icon='more_vert' />
          </PaperButton>
        </PaperToolbarTools>
      </PaperToolbar>

      <br />
      {{! with contextual components }}
      <PaperToolbar as |toolbar|>
        <toolbar.tools>
          <PaperButton>
            Go Back
          </PaperButton>
          <h2>Toolbar with Standard Buttons</h2>
          <span class='flex'></span>
          <PaperButton @raised={{true}}>
            Learn More
          </PaperButton>
          <PaperButton @mini={{true}} @aria-label='Favorite'>
            <PaperIcon @icon='favorite' />
          </PaperButton>
        </toolbar.tools>
      </PaperToolbar>

      <br />

      <PaperToolbar @tall={{true}} @accent={{true}}>
        <PaperToolbarTools>
          <h2>Toolbar: tall=true, accent=true</h2>
        </PaperToolbarTools>
      </PaperToolbar>

      <br />

      <PaperToolbar @tall={{true}} @warn={{true}}>
        <span class='flex'></span>
        <PaperToolbarTools>
          <h2>Toolbar: tall with actions pin to the bottom (tall=true warn=true)</h2>
        </PaperToolbarTools>
      </PaperToolbar>
    </card.content>
  </PaperCard>

  <PaperCard as |card|>
    <card.title>
      <h2>Tooltip</h2>
    </card.title>
    <card.content @class='layout layout-align-space-around'>
      <div class='layout-column layout-align-center-center'>
        <PaperButton @raised={{true}}>
          A button
          <PaperTooltip @position='bottom'>
            Hey! I'm a tooltip. Çup?
          </PaperTooltip>
        </PaperButton>
        <p><code>position: bottom</code></p>
      </div>

      <div class='layout-column layout-align-center-center'>
        <PaperButton @iconButton={{true}} @raised={{true}}>
          <PaperIcon @icon='create' />
          <PaperTooltip @position='top'>
            Create
          </PaperTooltip>
        </PaperButton>
        <p><code>position: top</code></p>
      </div>

      <div class='layout-column layout-align-center-center'>
        <PaperButton @fab={{true}} @raised={{true}} @accent={{true}}>
          <PaperIcon @icon='delete' />
          <PaperTooltip @position='left'>
            Delete
          </PaperTooltip>
        </PaperButton>
        <p><code>position: left</code></p>
      </div>

      <div class='layout-column layout-align-center-center'>
        <PaperIcon @icon='info'>
          <PaperTooltip @position='right'>
            Icon tooltip
          </PaperTooltip>
        </PaperIcon>
        <p><code>position: right</code></p>
      </div>
    </card.content>
  </PaperCard>

</DocContent>

{{#if this.dialogShowDialog}}
  <PaperDialog
    @class='flex-50'
    @clickOutsideToClose={{true}}
    @focusOnOpen={{false}}
    @onClose={{this.dialogToggleOpen}}
    @parent='#dialog-catalog'
  >
    <form>
      <PaperToolbar>
        <PaperToolbarTools>
          <h2>Mango (Fruit)</h2>
          <span class='flex'></span>
          <PaperButton @iconButton={{true}} @onClick={{this.dialogToggleOpen}}>
            <PaperIcon @icon='close' />
          </PaperButton>
        </PaperToolbarTools>
      </PaperToolbar>

      <PaperDialogContent>
        <p>
          This is a dialog rendered into a specific element. Clicking outside
          the dialog will close it.
        </p>
      </PaperDialogContent>

      <PaperDialogActions @class='layout-row'>
        <span class='flex'></span>
        <PaperButton @onClick={{this.dialogToggleOpen}}>
          Cancel
        </PaperButton>
        <PaperButton @onClick={{this.dialogToggleOpen}}>
          OK
        </PaperButton>
      </PaperDialogActions>
    </form>
  </PaperDialog>
{{/if}}