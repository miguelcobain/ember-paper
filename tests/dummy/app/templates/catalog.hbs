            <card.actions>
              <PaperButton>Action 1</PaperButton>
              <PaperButton>Action 2</PaperButton>
            </card.actions>
          </PaperCard>

          <PaperCard as |card|>
            <card.title as |title|>
              <title.text as |text|>
